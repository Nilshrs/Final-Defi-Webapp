

<main>

    <div class="container tbl-container">
        <div class="row tbl-fixed">
            <div class="table">
                <table class="table table-striped table-dark">
                    <thead class="sticky-top">
                    <tr class="info">
                        <th>Token</th>
                        <th>Currency</th>
                        <th>Price</th>
                        <th></th>
                    </tr>
                    </thead>

                    <tbody >
                    <% if (tokens) { %>
                        <% tokens.forEach( token => { %>
                            <tr>
                                <td>
                                    <%= token.name; %>
                                </td>
                                <td>
                                    <%= token.currency; %>
                                </td>
                                <td>
                                    <%= token.price; %>
                                </td>
                                <td>
                                    <a href="/trans-02/<%=token.id%>"> Add </a>
                                </td>
                            </tr>
                    <% }); }%>
                    </tbody>
                </table>
            </div>
        </div>
    </div>

</main>
