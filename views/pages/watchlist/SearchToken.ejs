 <div class="container">

        <a class=" text-white text-decoration-none" href="/watchlist"> ‚ùÆ </a>
        <h1>Result:</h1>

        <div class="row">
            <div class="table-responsive">
                <table id="tb" class="table table-striped table-dark">
                    <thead>
                    <tr class="info">
                        <th>Token</th>
                        <th>Currency</th>
                        <th>Price</th>
                        <th>ADD</th>
                    </tr>
                    </thead>
                    <tbody>
                    <% if (tokens) { %>
                        <% tokens.forEach((token) => { %>
                            <tr>
                                <td>
                                    <%= token.name %>
                                </td>
                                <td>
                                    <%= token.currency %>
                                </td>
                                <td>
                                    <%= token.price %>
                                </td>
                                <td>
                                    <a href='/watchlist/add-token/<%= token.id %> ' class="text-decoration-none"> <b style="color: deeppink"> + </b>  </a>
                                </td>
                            </tr>
                    <% }); } %>
                    </tbody>
                </table>
            </div>
        </div>
</div>
